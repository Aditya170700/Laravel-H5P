{
    "name": "adityaricki/laravel-h5p",
    "description": "An updated H5P integration for Laravel 10 & 11. Forked from brnysn/laravel-h5p.",
    "type": "package",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "bensampo/laravel-enum": "^6.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "h5p/h5p-core": "^1|dev-master",
        "h5p/h5p-editor": "^1|dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "laravel/legacy-factories": "^1.0.4",
        "guzzlehttp/guzzle": "^7"
    },
    "autoload": {
        "psr-4": {
            "Adityaricki\\LaravelH5P\\": "src",
            "Adityaricki\\LaravelH5P\\Database\\Factories\\": "database/factories",
            "Adityaricki\\LaravelH5P\\Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adityaricki\\LaravelH5P\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aditya Ricki",
            "email": "adityaric72@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Adityaricki\\LaravelH5P\\HeadlessH5PServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
