{
    "name": "brnysn/laravel-h5p",
    "description": "Headless H5P Laravel REST API",
    "type": "package",
    "require": {
        "php": ">=7.4",
        "bensampo/laravel-enum": ">=2.0",
        "laravel/framework": "^8|^9",
        "h5p/h5p-core": "^1|dev-master",
        "h5p/h5p-editor": "^1|dev-master",
        "escolalms/settings": "^0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "laravel/legacy-factories": "^1.0.4",
        "guzzlehttp/guzzle": "^7"
    },
    "autoload": {
        "psr-4": {
            "brnysn\\LaravelH5P\\": "src",
            "brnysn\\LaravelH5P\\Tests\\": "tests",
            "brnysn\\LaravelH5P\\Database\\Factories\\": "database/factories",
            "brnysn\\LaravelH5P\\Database\\Seeders\\": "database/seeders"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yasin Baran",
            "email": "brnysn@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "brnysn\\LaravelH5P\\HeadlessH5PServiceProvider"
            ]
        }
    }
}
